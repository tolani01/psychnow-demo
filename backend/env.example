# PsychNow Backend - Environment Variables
# Copy this file to .env and fill in your actual values

# App Settings
ENVIRONMENT=development
DEBUG=true
APP_NAME=PsychNow
API_VERSION=v1

# Security
SECRET_KEY=your-super-secret-key-change-this-in-production-min-32-chars
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Database (SQLite for local development)
DATABASE_URL=sqlite:///./psychnow.db

# OpenAI API
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_MODEL=gpt-4o-mini
OPENAI_MAX_TOKENS=2000

# CORS Settings
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000

# Provider Invite Code
PROVIDER_INVITE_CODE=PSYCHNOW-PROVIDER-2024

# File Storage
UPLOAD_DIR=./uploads
PDF_DIR=./pdfs

# Email Settings (Optional - for demo notifications)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-gmail-app-password
ADMIN_EMAIL=your-email@domain.com
FROM_EMAIL=noreply@psychnow.com

